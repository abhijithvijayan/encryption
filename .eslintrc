{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "arrow-body-style": ["error", "always"],
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}
